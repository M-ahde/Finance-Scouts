generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
}

model Achievement {
  id          String   @id @map("_id") @default(auto()) @db.ObjectId
  title       String
  date        String
  description String
}

model Workshop {
  id          String   @id @map("_id") @default(auto()) @db.ObjectId
  title       String
  date        String
  time        String
  location    String
  description String
}

model JoinForm {
  id         String @id @map("_id") @default(auto()) @db.ObjectId
  fullName   String
  email      String
  major      String
  year       String
  motivation String
}

model TeamMember {
  id    String @id @map("_id") @default(auto()) @db.ObjectId
  name  String
  role  String
  dept  String
}


model RoadmapPhase {
  id      String   @id @map("_id") @default(auto()) @db.ObjectId
  title   String
  period  String
  items   String[]
}
