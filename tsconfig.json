{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "outDir": "dist",
    "module": "ESNext",
    "target": "ES2022",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
